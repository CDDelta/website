.page-votes(style="display: none")
  .page-header: .row.align-items-center
    .col-auto
      .page-pretitle Votes
      .page-title DAOName

    .col-auto.ml-auto.d-print-none
      a.btn.btn-primary.d-none.d-sm-inline-block(href="#", data-toggle="modal", data-target="#modal-new-vote")
        i.icon(data-feather="plus") 
        | New Vote
      a.btn.btn-primary.d-sm-none.btn-icon(href="#", data-toggle="modal", data-target="#modal-new-vote", aria-label="New vote")
        i.icon(data-feather="plus")

  .dimmer.active
    .loader
    .dimmer-content
      .row.proposals
        .col-md-6
          .card
            .progress.progress-sm.card-progress(data-toggle="tooltip", data-placement="top", title="Vote ends in 40 blocks")
              .progress-bar(style="width: 40%", role="progressbar", aria-valuenow="40", aria-valuemin="0", aria-valuemax="100")
                span.sr-only 40% complete
            .card-body
              .float-right.stamp.bg-green.text-white
                i.icon(data-feather="users")
              .text-muted.font-weight-normal.mt-0.mb-3 Mint
              .mb-3
                h3.mb-0 Recipient
                <div class="d-flex lh-sm py-1 align-items-center">
                  <span class="avatar mr-2" style="background-image: url(data:image/png;base64,)"></span>
                  <div class="flex-fill">
                    <div class="strong">Cedrik</div>
                    <div class="text-muted text-h5">BPr7vrFduuQqqVMu_tftxsScTKUq9ke0rx4q5C9ieQU</div>
                  </div>
                </div>

              .mb-3
                h3.mb-0 Quantity
                p.text-muted 5,000 GARDEN

              .mb-3
                h3.mb-0 Note
                p.text-muted Neque porro quisquam est qui dolorem ipsum quia dolor sit amet, consectetur, adipisci velit...

              .mb-3
                .row.align-items-center
                  .col.text-muted Yes
                  .col-auto.ml-auto.text-muted 39%

                .progress
                  .progress-bar.bg-green(style="width: 39%")

              .mb-3
                .row.align-items-center
                  .col.text-muted No
                  .col-auto.ml-auto.text-muted 10%

                .progress
                  .progress-bar.bg-red(style="width: 10%")

            .card-footer
              .row.align-items-center
                .col 
                  a.btn.btn-danger(href="#") NO
                  a.btn.btn-success.ml-3(href="#") YES
                .col-auto.ml-auto
                  .avatar-list.avatar-list-stacked
                    span.avatar(style="background-image: url(./static/avatars/000m.jpg)")
                    span.avatar +5

